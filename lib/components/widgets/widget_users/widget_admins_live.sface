{#case Bonfire.UI.Me.WidgetUsersLive.list_admins() |> filter_empty([], :character)}
  {#match []}
  {#match admins}
    <Bonfire.UI.Me.WidgetUsersLive
      :if={@current_user || Settings.get([Bonfire.UI.Me.WidgetAdminsLive, :show_guests], true, @__context__)}
      widget_title={e(@widget_title, l("Admins"))}
      users={admins}
    />
{/case}